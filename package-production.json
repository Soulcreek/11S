{
  "name": "11seconds-production",
  "version": "1.0.0",
  "description": "Production build for 11Seconds Quiz Game",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "build": "npm run build:web && npm run build:api",
    "build:web": "cd web && npm ci && npm run build",
    "build:api": "echo 'API build completed'",
    "deploy": "node AUTO-DEPLOY-MK/mk_deploy-cli.js --project web --target production --parts default --build --force"
  },
  "dependencies": {
    "express": "^4.18.2",
    "mysql2": "^3.6.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "sqlite3": "^5.1.6",
    "basic-ftp": "^5.0.3",
    "js-yaml": "^4.1.0",
    "node-fetch": "^2.6.7",
    "fast-glob": "^3.3.1",
    "commander": "^9.4.1"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "keywords": ["react", "quiz", "game", "production"],
  "author": "11Seconds Team",
  "license": "MIT"
}